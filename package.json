{
  "name": "nuxt-hotjar",
  "version": "0.0.0",
  "description": "Nuxt hotjar plugin",
  "author": {
    "name": "Neveena",
    "email": "neveena.ferrao@netsells.co.uk"
  },
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "keywords": [
    "Vue",
    "Nuxt",
    "Hotjar",
    "nuxt-hotjar"
  ],
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "prettier": "^1.15.2",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-config-xo": "^0.25.0",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "neveena/nuxt-hotjar",
  "jest": {
    "testEnvironment": "node"
  },
  "license": "MIT"
}
